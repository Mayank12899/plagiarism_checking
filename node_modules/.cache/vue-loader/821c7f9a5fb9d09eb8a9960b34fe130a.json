     subjectId: \"S1\",\n      },\n      {\n        subject: \"DLDA\",\n        class: \"SE COMPUTERS\",\n        date: Date.now(),\n        subjectId: \"S2\",\n      },\n      {\n        subject: \"Algorithms\",\n        class: \"SE COMPUTERS\",\n        date: Date.now(),\n        subjectId: \"S3\",\n      },\n    ],\n    courses: [],\n  }),\n  methods: {\n    add() {\n      this.subjects.push({\n        subject: this.subject,\n        class: this.myclass,\n        date: Date.now(),\n        subjectId: this.subjectId,\n      });\n      this.dialog = false;\n    },\n    reset() {\n      (this.subject = \"\"), (this.subjectId = \"\"), (this.myclass = \"\");\n      this.dialog = false;\n    },\n    remove() {\n      this.subjects.pop();\n    },\n    opentopic(id) {\n      this.$router.push({\n        name: \"Mytopic\",\n        params: { id: id },\n      });\n    },\n  },\n  created() {\n    db.collection(\"Subjects\")\n      .where(\"teacher\", \"==\", this.teacher)\n      .get()\n      .then((querySnapshot) => {\n        querySnapshot.forEach((doc) => {\n          let data = {\n            subject: doc.id,\n            branch: doc.data().Branch,\n            class: doc.data().Class,\n            id: doc.data().Id,\n          };\n          this.courses.push(data);\n        });\n      });\n  },\n};\n</script>\n"]}]}